<div class="gameContainer">
	<div class="maxHeight valign-wrapper">
		<table class="playingGrid z-depth-2">
			<% for i in 0..5 %>
				<tr>
				<% for c in 0..6 %>
					<td<% if $g.isFilled(c, i) %> class="filled <%= $g.getcolumnvalue(c, i) %>"<% end %>></td>
				<% end %>
				</tr>
			<% end %>
		</table>
	</div>
</div>
